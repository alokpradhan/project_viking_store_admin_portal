<h1>Viking Store Admin Portal</h1>

<nav>  
  <ul class="nav nav-pills nav-stacked">
    
    <h4>Navigation Options</h4>
    <li>Users</li>
    <li>Addresses</li>
    <li>Orders</li>
    <li>Products</li>
    <li>Categories</li>
    <li><%= link_to "Analytics Dashboard", root_path%></li>

    
</ul>

</nav>


<div class='container'>

  <div class='row'>

   <table class="table table-striped">
  <h4>Viking Store Users</h4>
  <%= link_to "Create a New user", new_user_path %>
  <tr>
    <%@headers.each do |head| %>
    <td> <%= head %> </td>
    <% end %>
  <tr>
    <% User.all.each do |user|%>
      <tr>
        <td> <%= user.id %></td>
        <td> <%= "#{user.first_name} #{user.last_name}" %></td>
        <td> <%= user.created_at %></td>
        
      <tr>
      <%end%>
</table>

  </div>

